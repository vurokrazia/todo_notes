<div class="my-5">
  <div class="card px-2">
    <h1>Notes</h1>
    <p style="color: green"><%= notice %></p>
    <%= link_to "New note", new_note_path, class:"btn btn-primary my-2"  %>
  </div>
  <div id="notes">
    <% @notes.each do |note| %>
      <%= render note %>
    <% end %>
  </div>
</div>
